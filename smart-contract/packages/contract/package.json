{
  "name": "@freed/contract",
  "description": "Smart contracts for Freed",
  "version": "1.0.0",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.2",
    "@tanstack/router-vite-plugin": "^1.31.18",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "ethers": "^6.9.0",
    "fs-extra": "^11.2.0",
    "hardhat": "^2.19.2",
    "hardhat-deploy": "^0.12.1",
    "hardhat-ethernal": "^3.2.5",
    "hardhat-gas-reporter": "^1.0.9",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.2.0",
    "shadcn-ui": "^0.8.0",
    "solhint": "^4.0.0",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.9.3",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3",
    "vite": "^5.2.11"
  },
  "files": [
    "contracts"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "pnpm run node & pnpm run api:dev",
    "api:dev": "tsx --watch src/api/run.ts",
    "clean": "rm -rf ./artifacts ./cache ./coverage ./types ./coverage.json && pnpm run typechain",
    "compile": "hardhat compile",
    "node": "hardhat node",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && pnpm run typechain",
    "deploy": "hardhat deploy",
    "lint": "pnpm run lint:sol && pnpm run lint:ts && pnpm run prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postcompile": "pnpm run typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "task:deployFreed": "hardhat task:deployFreed",
    "test": "hardhat test",
    "typechain": "hardhat typechain"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.1",
    "@hono/zod-validator": "^0.2.1",
    "@openzeppelin/contracts": "^5.0.2",
    "@t3-oss/env-core": "^0.3.1",
    "zod": "^3.23.6"
  }
}
